
# Timeline es un contenedor que generará un único vídeo como salida

%timeline{width: 1280, height: 720, videocodec: libtheora, audiocodec: flac} surface.mkv
  # Cada marca se puede delimitar
  %video{from: 7, to: 10, volume: 0} surface.mp4
  %video{from: 12, to: 15, volume: 0} surface.mp4

  %audio{start: 0, to: 12, volume: 90} icarus.flac
  
